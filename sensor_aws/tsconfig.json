{
  "compilerOptions": {
    "target":"ES2018",
    "module": "commonjs",
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "strictPropertyInitialization":true,
    "removeComments": true,
    "baseUrl": "./",
    "incremental": true
  },
  "exclude": ["cdk.out", "test"]
}
